generator client {
  provider = "prisma-client"
  output   = "../generated/prisma"
}

datasource db {
  provider = "postgresql"
}

model Company {
  name             String
  furigana         String?
  kind             String
  post_code        String?
  prefecture_name  String?
  city_name        String?
  street_number    String?
  assignment_date  DateTime   @db.Date
  corporate_number String     @id

  @@map("companies")
}